</div>
<div class="form-group">
	<label for="Partner1AddressProvince">Province</label>
	@Html.DropDownListFor(m => m.Partner1AddressProvince, new SelectList(ViewBag.Provinces), new { @class = "form-control", placeholder = "Province" })
</div>
<div class="form-group">
	<label for="Partner1AddressCountry">Country</label>
