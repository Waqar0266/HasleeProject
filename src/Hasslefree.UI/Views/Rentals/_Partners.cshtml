</div>
<div class="form-group">
	<label for="Partner1Address1">Address 1</label>
	@Html.TextBoxFor(m => m.Partner1Address1, new { @class = "form-control", @placeholder = "Address 1", google_address = "true" })
</div>
<div class="form-group">
	<label for="Partner1Address2">Address 2</label>
	@Html.TextBoxFor(m => m.Partner1Address2, new { @class = "form-control", @placeholder = "Address 2" })
