@model Hasslefree.Web.Models.Rentals.CompleteRental

<h3 class="subheadline">Details of Shareholders of more than 25%</h3>
<h3 class="subheadline">Shareholder 1</h3>
<div class="form-group">
	<label for="Partner1Name">Name</label>
	@Html.TextBoxFor(m => m.Partner1Name, new { @class = "form-control", @placeholder = "Name" })
</div>
<div class="form-group">
	<label for="Partner1Surname">Surname</label>
	@Html.TextBoxFor(m => m.Partner1Surname, new { @class = "form-control", @placeholder = "Surname" })
</div>
