</div>
<div class="form-group">
	<label for="Partner1Nationality">Nationality</label>
	@Html.TextBoxFor(m => m.Partner1Nationality, new { @class = "form-control", @placeholder = "Nationality" })
</div>
<div class="form-group">
	<label for="Partner1Email">Email</label>
	@Html.TextBoxFor(m => m.Partner1Email, new { @class = "form-control", @placeholder = "Email" })
</div>
<div class="form-group">
	<label for="Partner1Phone">Home Tel</label>
